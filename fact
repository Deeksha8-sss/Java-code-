import java.util.*;
public class Main{
    public int fact(int n){
        if(n==0||n==1){
            return 1;
        }else{
            return  n*fact(n-1);
        }
    }
    public static void main(String[]agrs){
        Scanner sc=new Scanner(System.in);
        Main obj=new Main();
        int n=sc.nextInt();
        System.out.println(obj.fact(n));
    }
}
