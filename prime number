import java.util.Scanner;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int i;

        if (num <= 1) {
            System.out.println(num + " is not a Prime Number");
        } else {
            
            for (i = 2; i < num; i++) {
                if (num % i == 0) {
                
                }
            }
            if (i == num) {
                System.out.println(num + " is a Prime Number");
            } else {
                System.out.println(num + " is not a Prime Number");
            }
        }

    }
}
